    "@prisma/client": "^4.4.0",
    "ajv": "^8.11.0",
    "ajv-i18n": "^4.2.0",
    "aws-sdk": "^2.1233.0",
    "ramda": "^0.28.0"
  },
  "devDependencies": {
    "aws-info": "^1.2.0",
    "serverless": "^3.23.0",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-offline": "^8.8.0"
  }
