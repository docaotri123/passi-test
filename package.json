  "license": "ISC",
  "dependencies": {
    "ajv": "^8.11.0",
    "ajv-i18n": "^4.2.0",
    "aws-sdk": "^2.1220.0",
    "ramda": "^0.28.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.104",
    "@types/node": "^18.7.18",
    "aws-info": "^1.2.0",
    "esbuild": "^0.15.8",
    "serverless": "^3.22.0",
    "serverless-esbuild": "^1.32.8",
    "serverless-offline": "^8.8.0"
  }
}
