{
  "name": "passii",
  "version": "1.0.0",
  "scripts": {
    "offline": "sls offline start --stage local --httpPort 5000 --aws-profile dctri-user-admin",
    "seed:local": "node seeds stage=local",
    "seed:dev": "sls invoke -f seed --stage dev --aws-profile dctri-user-admin",
    "pre:deploy:dev": "npx prisma generate --schema=.esbuild/.build/src/functions/v1/schema.prisma",
    "deploy:dev": "sls deploy --stage dev --aws-profile dctri-user-admin --verbose",
    "remove:dev": "sls remove --stage dev --aws-profile dctri-user-admin --verbose"
  },
