{
  "name": "passii",
  "version": "1.0.0",
  "scripts": {
    "offline": "sls offline start --stage local --httpPort 5000 --aws-profile dctri-user-admin",
    "seed:local": "node seeds stage=local",
    "seed:dev": "sls invoke -f seed --stage dev --aws-profile dctri-user-admin",
    "deploy:dev:v1": "sls deploy --stage dev1 --aws-profile dctri-user-admin --verbose",
    "log:dev": "sls logs --stage dev1 --aws-profile dctri-user-admin --verbose --function triggerSendOTP --tail",
    "warmup": "sls invoke local -f warmUpFunc --stage local --aws-profile dctri-user-admin"
  },
