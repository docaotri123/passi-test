{
    "name": "passii",
    "version": "1.0.0",
    "scripts": {
        "format:check": "prettier --check .",
        "format:write": "prettier --write .",
        "lint:check": "eslint .",
        "lint:fix": "eslint --fix .",
        "offline": "sls offline --stage local --httpPort 5000 --aws-profile dctri-user-admin",
        "seed:create": "./bin/init_migration.sh",
        "seed:local": "node seeds stage=local",
        "seed:dev": "sls invoke -f seed --stage dev --aws-profile dctri-user-admin",
        "deploy:dev:v3": "npm run generate-client && sls deploy --stage dev3 --aws-profile dctri-user-admin --verbose",
        "generate-client": "npx prisma generate --data-proxy",
        "migrate": "DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma migrate deploy"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^4.3.1",
        "ajv": "^6.12.6",
        "ajv-i18n": "^3.5.0",
        "js-yaml": "^4.1.0",
        "moment": "^2.29.3",
        "ramda": "^0.28.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": [
            "prettier --write",
            "git add"
        ]
    },
    "devDependencies": {
        "aws-info": "^1.2.0",
        "eslint": "^8.19.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-node": "^4.1.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "prettier": "^2.7.1",
        "prisma": "^4.3.0",
        "serverless-offline": "^8.8.0"
    }
}
