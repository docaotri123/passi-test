{
    "name": "passii",
    "version": "1.0.0",
    "scripts": {
        "offline": "sls offline --stage local --httpPort 5000 --aws-profile dctri-user-admin",
        "seed:local": "node seeds stage=local",
        "seed:dev": "sls invoke -f seed --stage dev --aws-profile dctri-user-admin",
        "deploy:dev:v3": "npm run generate-client && sls deploy --stage dev3 --aws-profile dctri-user-admin --verbose",
        "build": "tsc -p tsconfig.build.json",
        "prettier-format": "prettier --config .prettierrc .ts --write",
        "lint": "eslint . --ext .ts",
        "lint-and-fix": "eslint . --ext .ts --fix"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "ajv": "^8.11.0",
        "ajv-i18n": "^4.2.0",
        "aws-sdk": "^2.1220.0",
        "ramda": "^0.28.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": [
            "prettier --write",
            "git add"
        ]
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.104",
        "@types/node": "^18.7.18",
        "@types/ramda": "^0.28.15",
        "@typescript-eslint/eslint-plugin": "^5.38.0",
        "@typescript-eslint/parser": "^5.38.0",
        "aws-info": "^1.2.0",
        "eslint": "^8.23.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "prettier": "^2.7.1",
        "serverless": "^3.22.0",
        "serverless-offline": "^8.8.0",
        "serverless-plugin-typescript": "^2.1.2",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.3"
    }
}
